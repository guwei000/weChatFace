.pluginContent {
  position: absolute;
  top: 0;
  margin: 20rpx;
  width: calc(100vw - 40rpx);
  height: calc(100vh - 110rpx);
  background-color: #fff;
  text-align: center;
  box-shadow: 0px 0px 19px 1px rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
}

.img-wrap {
  position: relative;
  margin: 189rpx auto 64rpx;
  width: 78rpx;
  height: 78rpx;
}

.img {
  width: 100%;
  height: 0;
}

.success-title {
  font-size: 36rpx;
  font-weight: 700;
  margin-bottom: 42rpx;
}

.success-des {
  margin-bottom: 69rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.btn {
  width: 198rpx;
  height: 86rpx;
  font-size: 32rpx;
  color: #419FFF !important;
  border: 2rpx solid #419FFF !important;
}
